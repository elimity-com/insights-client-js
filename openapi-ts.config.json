{
  "input": "/root/swagger.json",
  "output": {
    "path": "openapi",
    "postProcess": ["prettier"]
  },
  "parser": {
    "filters": {
      "operations": {
        "include": [
          "POST /sources/{id}/connector-logs",
          "POST /sources/{id}/snapshots"
        ]
      }
    }
  },
  "plugins": [
    {
      "name": "@hey-api/sdk",
      "operations": {
        "strategy": "single"
      }
    },
    {
      "dates": true,
      "name": "@hey-api/transformers"
    }
  ]
}
